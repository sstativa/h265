{
    "env": {
        "node": true,
        "es2021": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "rules": {
        "max-len": ["error", { "code": 180 }],
        "no-console": 0,
        "no-underscore-dangle": 0
    }
}
